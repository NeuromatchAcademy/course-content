{
  "plugins": {},
  "filters": {},
  "rules": {
    "@textlint-rule/textlint-rule-no-unmatched-pair": true,
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": true,
    "@textlint-ja/textlint-rule-no-synonyms": true,
    "textlint-rule-no-mix-dearu-desumasu": true,
    "textlint-rule-no-double-negative-ja": true,
    "textlint-rule-ja-no-abusage": true,
    "textlint-rule-ja-no-redundant-expression": true,
    "textlint-rule-no-dropping-the-ra": true,
    "textlint-rule-no-doubled-conjunctive-particle-ga": true,
    "textlint-rule-no-doubled-conjunction": true,
    "textlint-rule-incremental-headers": true,
    "textlint-rule-terminology": true,
    "textlint-rule-footnote-order": true,
    "textlint-rule-ja-hiragana-hojodoushi": true,
    "textlint-rule-prefer-tari-tari": true,
    "textlint-rule-preset-ja-spacing": true,
    "textlint-rule-prh": {
      "rulePaths": ["./prh.yaml"]
    }
  }
}